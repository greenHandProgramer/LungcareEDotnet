                    <Button x:Name="btnADMIN" Grid.ColumnSpan="2" Content="ADMIN" HorizontalAlignment="Left" Margin="652,9,0,0" VerticalAlignment="Top" Width="75"  Visibility="Hidden"/>

                </Grid>
            </Border>
